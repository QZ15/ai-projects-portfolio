  "scripts": {
    "dev:glaze:frontend": "cd apps/glaze/frontend && npm run dev",
    "dev:glaze:backend": "cd apps/glaze/backend && npm run dev",
    "dev:glaze": "concurrently \"npm run dev:glaze:frontend\" \"npm run dev:glaze:backend\"",
    "dev:portfolio:frontend": "cd apps/portfolio/frontend && npm run dev",
    "dev:portfolio:backend": "cd apps/portfolio/backend && npm run dev",
    "dev:portfolio": "concurrently \"npm run dev:portfolio:frontend\" \"npm run dev:portfolio:backend\"",
    "build:glaze:frontend": "cd apps/glaze/frontend && npm run build",
    "build:portfolio:frontend": "cd apps/portfolio/frontend && npm run build",
    "build": "concurrently \"npm run build:glaze:frontend\" \"npm run build:portfolio:frontend\"",
    "build-all": "npm run build -w apps/portfolio/frontend && npm run build -w apps/glaze/frontend && node copy-static.js",
    "start:glaze:backend": "cd apps/glaze/backend && npm start",
    "start:portfolio:backend": "cd apps/portfolio/backend && npm start",
    "lint": "eslint apps/nutfit/src/utils functions/src/routes/progressFunctions.ts functions/src/routes/progressFunctions.test.ts apps/nutfit/src/utils/format.test.ts",
    "lint:fix": "eslint apps/nutfit/src/utils functions/src/routes/progressFunctions.ts functions/src/routes/progressFunctions.test.ts apps/nutfit/src/utils/format.test.ts --fix",
    "test": "jest"
  },
