# Example shared environment variables
# Copy to .env and fill in your secrets. Firebase and Stripe keys are prefixed
# with VITE_ so they can be exposed to the browser where needed. Leave
# OPENAI_API_KEY unprefixed so it remains server-only.
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FUNCTIONS_BASE_URL=http://localhost:5001/project/us-central1
VITE_PRICE_ID_MONTHLY=
VITE_PRICE_ID_YEARLY=
OPENAI_API_KEY=
